{
  "name": "zabbix-promise",
  "version": "1.0.5",
  "description": "Simplified Zabbix API Client with Promise support",
  "main": "index.js",
  "scripts": {
    "doc": "./node_modules/.bin/jsdox -o docs index.js",
    "test": "bash ./tasks/test.sh",
    "update": "./node_modules/.bin/ncu -u && rm -rf ./node_modules yarn.lock && yarn",
    "commit": "./node_modules/.bin/git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitgoelpw/zabbix-promise.git"
  },
  "keywords": [
    "Zabbix",
    "API"
  ],
  "author": {
    "name": "Sumit Goel",
    "email": "sumit@goel.pw",
    "url": "https://sumit.goel.pw"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sumitgoelpw/zabbix-promise/issues"
  },
  "homepage": "https://github.com/sumitgoelpw/zabbix-promise#readme",
  "dependencies": {
    "request": "2.81.0",
    "request-promise-native": "1.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-sumitgoelpw": "1.0.1",
    "jsdox": "0.4.10",
    "mocha": "3.2.0",
    "npm-check-updates": "2.10.4",
    "nsp": "2.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "sumitgoelpw"
  }
}
